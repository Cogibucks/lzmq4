<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="lzmq4" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="lzmq4.so" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="tests.lua" />
				<Option host_application="lua" />
				<Option run_host_application_in_terminal="1" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="lzmq4.so" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="0" extension_auto="0" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
		</Compiler>
		<Linker>
			<Add library="lua" />
			<Add library="zmq" />
		</Linker>
		<Unit filename="src/lzmq-context.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/lzmq-message.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/lzmq-socket.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/lzmq.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="src/lzmq.h" />
		<Unit filename="tests.lua" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
